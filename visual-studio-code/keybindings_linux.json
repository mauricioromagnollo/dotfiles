[
  {
    "key": "alt+shift+d",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+shift+alt+down",
    "command": "-editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+[BracketLeft]",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "alt+shift+[BracketLeft]",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "alt+shift+t",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+shift+5",
    "command": "-workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+alt+r",
    "command": "editor.action.reindentlines"
  },
  {
    "key": "alt+shift+w",
    "command": "workbench.action.closeAllGroups"
  },
  {
    "key": "alt+shift+alt+c",
    "command": "workbench.action.openQuickChat.copilot"
  },
  {
    "key": "alt+shift+p",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "alt+b",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "alt+c",
    "command": "editor.action.clipboardCopyAction"
  },
  {
    "key": "alt+x",
    "command": "editor.action.clipboardCutAction"
  },
  {
    "key": "alt+v",
    "command": "editor.action.clipboardPasteAction"
  },
  {
    "key": "alt+s",
    "command": "chatEditing.saveAllFiles",
    "when": "hasUndecidedChatEditingResource && inChatInput && !chatSessionRequestInProgress && chatLocation == 'editing-session'"
  },
  {
    "key": "alt+s",
    "command": "workbench.action.files.save"
  },
]
